    <TextView
        android:id="@+id/LinearLayouttxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INI LINEAR LAYOUT"
        android:textStyle="bold"
        android:textSize="16sp"
        android:layout_marginBottom="20dp"
        />

    <LinearLayout
        android:layout_below="@+id/LinearLayouttxt"
        android:id="@+id/LinearLayout"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_marginEnd="20dp"
            android:layout_marginTop="20dp"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:src="@drawable/ic_launcher_background"

            />

        <ImageView
            android:layout_marginTop="20dp"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:src="@drawable/ic_launcher_background"


            />

    </LinearLayout>

    <RelativeLayout
        android:layout_marginTop="20dp"
        android:layout_below="@id/LinearLayout"
        android:id="@+id/RelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="INI RELATIVE LAYOUT"
            android:textSize="16sp"
            android:textStyle="bold"

            />
        <ImageView
            android:id="@+id/imageView1"
            android:layout_below="@id/textView"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_launcher_background" />

        <ImageView
            android:id="@+id/imageview2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@id/imageView1"
            android:layout_centerInParent="true"
            android:src="@drawable/ic_launcher_background" />


        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_below="@id/imageview2"
            android:layout_centerInParent="true"

            android:src="@drawable/ic_launcher_background"

            />



    </RelativeLayout>



    <TextView
        android:layout_below="@id/RelativeLayout"
        android:id="@+id/txtTable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INI TABLE LAYOUT"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        />


    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_below="@id/txtTable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama"

                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Umur"
                />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:hint="Mahda Laina"

                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="20"
                />
        </TableRow>

        <TextView
            android:id="@+id/txtFrame"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="INI FRAME LAYOUT"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_below="@id/tableLayout"
            android:textColor="@color/black"

            />
        <FrameLayout
            android:layout_below="@id/tableLayout"
            android:layout_width="match_parent"
            android:id="@+id/frameLayout"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/ic_launcher_background"
                android:layout_gravity="center"/>
            <Button
            android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="Login"
                android:layout_gravity="center"

                />
        </FrameLayout>


    </TableLayout>


</RelativeLayout>
